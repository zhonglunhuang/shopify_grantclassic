<script>
	(() => {
		class VtlsCompareSlider extends HTMLElement {
			constructor() {
				super();
				this.isDragging = false;
			}

			connectedCallback() {
				this.setHTMLelements();
				this.setEventListeners();
				this.initSlider();
			}

			setHTMLelements() {
				this.leftImage = this.querySelector('.VtlsComSlider__Image--left');
				this.rightImage = this.querySelector('.VtlsComSlider__Image--right');
				this.sliderNav = this.querySelector('.VtlsTextSliderNav');
				this.circle = this.querySelector('.VtlsTextSliderNav__Circle');
				this.border = this.querySelector('.VtlsTextSliderNav__Border');
			}

			setEventListeners() {
				this.sliderNav.addEventListener('mousedown', this.startDrag.bind(this));
				window.addEventListener('mousemove', this.onDrag.bind(this));
				window.addEventListener('mouseup', this.stopDrag.bind(this));

				this.sliderNav.addEventListener('touchstart', this.startDrag.bind(this));
				window.addEventListener('touchmove', this.onDrag.bind(this));
				window.addEventListener('touchend', this.stopDrag.bind(this));
				
				this.addEventListener('keydown', this.handleKeyboard.bind(this));
				window.addEventListener('resize', this.handleResize.bind(this));
			}

			initSlider() {
				this.currentPosition = 50;
				this.updateSliderPosition(50);
				this.updateAriaAttributes();
			}

			updateAriaAttributes() {
				const bounds = this.calculateDynamicBounds();
				this.circle.setAttribute('aria-valuemin', Math.round(bounds.min));
				this.circle.setAttribute('aria-valuemax', Math.round(bounds.max));
			}

			handleResize() {
				// Update ARIA attributes when container resizes
				this.updateAriaAttributes();
				
				// Ensure current position stays within new bounds
				const bounds = this.calculateDynamicBounds();
				const constrainedPosition = Math.max(bounds.min, Math.min(bounds.max, this.currentPosition));
				if (constrainedPosition !== this.currentPosition) {
					this.updateSliderPosition(constrainedPosition);
				}
			}

			calculateDynamicBounds() {
				const containerWidth = this.getBoundingClientRect().width;
				const buttonWidth = this.circle.offsetWidth;
				
				// Calculate the minimum and maximum percentage where the button stays fully visible
				const minPercentage = (buttonWidth / 2) / containerWidth * 100;
				const maxPercentage = 100 - (buttonWidth / 2) / containerWidth * 100;
				
				return {
					min: Math.max(0, minPercentage),
					max: Math.min(100, maxPercentage)
				};
			}
			
			handleKeyboard(e) {
				const bounds = this.calculateDynamicBounds();
				
				if (e.key === 'ArrowLeft') {
					e.preventDefault();
					this.updateSliderPosition(Math.max(bounds.min, this.currentPosition - 5));
				} else if (e.key === 'ArrowRight') {
					e.preventDefault();
					this.updateSliderPosition(Math.min(bounds.max, this.currentPosition + 5));
				} else if (e.key === 'Home') {
					e.preventDefault();
					this.updateSliderPosition(bounds.min);
				} else if (e.key === 'End') {
					e.preventDefault();
					this.updateSliderPosition(bounds.max);
				}
			}

			startDrag(e) {
				this.isDragging = true;
				this.updateSlider(e);
			}

			onDrag(e) {
				if (!this.isDragging) {
					return;
				}

				this.updateSlider(e);
			}

			stopDrag() {
				this.isDragging = false;
			}

			updateSlider(e) {
				const rect = this.getBoundingClientRect();
				const xPos = e.touches ? e.touches[0].clientX : e.clientX;
				let percentage = ((xPos - rect.left) / rect.width) * 100;

				const bounds = this.calculateDynamicBounds();
				percentage = Math.max(bounds.min, Math.min(bounds.max, percentage));
				this.updateSliderPosition(percentage);
			}

			updateSliderPosition(percentage) {
				this.currentPosition = percentage;
				this.leftImage.style.clipPath = `inset(0 ${100 - percentage}% 0 0)`;
				this.circle.style.left = `${percentage}%`;
				this.border.style.left = `${percentage}%`;
				
				// Update ARIA attributes
				this.circle.setAttribute('aria-valuenow', Math.round(percentage));
			}
		}

		document.addEventListener('shopify:section:load', () => {
			if (!customElements.get('vtls-compare-slider-simple')) {
				customElements.define('vtls-compare-slider-simple', VtlsCompareSlider);
			}
		});

		document.addEventListener('DOMContentLoaded', () => {
			if (!customElements.get('vtls-compare-slider-simple')) {
				customElements.define('vtls-compare-slider-simple', VtlsCompareSlider);
			}
		});
	})();
</script>

<script>
	(function () {
		const uninstallMessage = 'Displaying this section requires an active Vitals subscription. Reinstall the <a href="https://apps.shopify.com/vitals" target="_blank">Vitals app</a> to activate it or consider removing the section.';
		const downgradeMessage = 'You downgraded to a free plan. You need to upgrade your <a href="https://apps.shopify.com/vitals" target="_blank">Vitals app</a> plan to activate it or consider removing the section.';
		const projectId = '1';
		const sectionsSelector = '.shopify-section';
		const apps = {
			'1': {
				prefix: 'vitals',
				hostname: 'https://appsolve.io'
			},
			'5': {
				prefix: 'tsapb',
				hostname: 'https://theme-sections.sqa-api.com'
			}
		};

		const cacheKeysMetaFields = '{{ shop.metafields.vitals.tsck }}';
		const shopId = '{{ shop.id }}';

		const getHostname = () => {
			if (apps[projectId].hostname) {
				return apps[projectId].hostname;
			}

			console.error(`hostname for appId ${projectId} cannot be determined.`);
			return undefined;
		}

		const disableSection = (message, selector) => {
			const mainContainer = document.querySelector(selector);
			const contentContainer = document.querySelector(`${selector} > section`);

			if ('{{ request.design_mode }}' === 'true') {
				contentContainer.innerHTML = `<h2 class="VtlsUninstallBG"><div class="VtlsUninstallBG__Text">${message}</div></h2>`;
			} else {
				mainContainer.style.display = 'none';
				mainContainer.remove();
			}
		};

		const getThemeSectionIdsByUUID = (installedThemeSections, uuid, appId) => {
			const listIds = [];

			installedThemeSections.forEach((installedThemeSection) => {
				if (installedThemeSection.id.includes(`${apps[appId].prefix}_${uuid}`)) {
					listIds.push(installedThemeSection.id);
				}
			})

			return listIds;
		}

		const getThemeSectionsStatusList = (url) => fetch(url, {
			method: 'GET',
			headers: { 'Content-Type': 'application/json' },
		}).then((response) => response.json());

		const evaluateThemeSections = (url) => {
			getThemeSectionsStatusList(url).then(response => {
				const { data, f, i } = response;
				const installedThemeSections = document.querySelectorAll(sectionsSelector);

				data.forEach((themeSection) => {
					const { e, u, a } = themeSection;

					if (!e) {
						const themeSectionsWithUUID = getThemeSectionIdsByUUID(installedThemeSections, u, a);
						let message = '';

						if (f) {
							message = downgradeMessage;
						}

						if (!i) {
							message = uninstallMessage;
						}

						themeSectionsWithUUID.forEach((themeSectionSelector) => {
							disableSection(message, `#${themeSectionSelector}`);
						})
					}
				});
			})
		}

		if (shopId) {
			if (window.vtlsSecurityCheckStarted === undefined) {
				window.vtlsSecurityCheckStarted = true;

				let url = '';
				const hostname = getHostname();

				if (!hostname) {
					return;
				}

				const baseUrl = `${hostname}/bundle/api/v2/sf/ts`;

				if (cacheKeysMetaFields) {
					url = `${baseUrl}/${shopId}/${cacheKeysMetaFields}.json`;
				} else {
					const timestamp = Math.round(new Date().getTime() / 1000 / 10000) * 10000;
					url = `${baseUrl}/${shopId}/${timestamp}.json`;
				}

				evaluateThemeSections(url);
			}
		}
	})();
</script>

