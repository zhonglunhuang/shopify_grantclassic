<!-- --UUID-8563bf27-- -->

<section class="Vtls-{{ section.id | handle }}" {% if section.settings.heading != blank %}aria-labelledby="section-heading-{{ section.id | handle }}"{% else %}aria-label="Image comparison section"{% endif %}>
	{%- capture card_image_background -%}
		{%- case section.settings.section_background -%}
			{%- when '' or 'rgba(0,0,0,0)' -%}
				true
			{%- else -%}
				false
		{%- endcase -%}
	{%- endcapture -%}
	<div
		class="
			VtlsComSliderContainer
			{% if section.settings.background_image == blank and section.settings.section_background_style == 'image' %}VtlsComSliderContainer--emptyImage{% endif %}
			VtlsComSliderContainer--backgroundStyle-{{ section.settings.section_background_style }}

		"
	>
		<div class="VtlsComSlider 
					VtlsComSlider--desktopPosition-{{ section.settings.image_position }} 
					VtlsComSlider--mobilePosition-{{ section.settings.image_mobile_position }}
					{% if section.settings.section_width == 100 and section.settings.section_max_width == blank %}VtlsComSlider--fullWidth{% endif %}
					{% if section.settings.section_max_width != blank and section.settings.section_width == 100 %}VtlsComSlider--customWidth{% endif %}">
			<div class="VtlsComSlider__Heading">
				{%- if section.settings.heading != blank -%}
					<h2 id="section-heading-{{ section.id | handle }}" class="VtlsComSliderHeading VtlsComSliderHeading--style-{{ section.settings.heading_style }}">
						{{ section.settings.heading }}
					</h2>
				{%- endif -%}
			</div>
			<vtls-compare-slider-simple class="VtlsComSlider__Media">
				<div class="VtlsComSlider__Image {% if card_image_background == 'true' %}VtlsComSlider__Image--hasColor{% endif %}">
					{%- if section.settings.left_image != blank -%}
						{{-
							section.settings.left_image
							| image_url: width: section.settings.left_image.width
							| image_tag:
								sizes: "100vw",
								widths: "400, 600, 800, 1200, 1600, 1800, 2400, 2800, 3200",
								fetchpriority: "low",
								loading: "lazy",
								class: "VtlsComSlider__Image--left"
						-}}
					{%- else -%}
						<img
							src="data:image/webp;base64,"
							alt="Before image for comparison"
							width="720"
							height="400"
							loading="lazy"
							sizes="100vw"
							fetchpriority="low"
							class="VtlsComSlider__Image--left"
						>
					{%- endif -%}
					{%- if section.settings.right_image != blank -%}
						{{-
							section.settings.right_image
							| image_url: width: section.settings.right_image.width
							| image_tag:
								sizes: "100vw",
								widths: "400, 600, 800, 1200, 1600, 1800, 2400, 2800, 3200",
								fetchpriority: "low",
								loading: "lazy",
								class: "VtlsComSlider__Image--right"
						-}}
					{%- else -%}
						<img
							src="data:image/webp;base64,"
							alt="After image for comparison"
							width="720"
							height="400"
							loading="lazy"
							sizes="100vw"
							fetchpriority="low"
							class="VtlsComSlider__Image--right"
						>
					{%- endif -%}
				</div>
				<div class="VtlsTextSliderNav">
					<div class="VtlsTextSliderNav__Border" aria-hidden="true">&#8203;</div>
					<button 
						class="VtlsTextSliderNav__Circle"
						role="slider"
						aria-label="Drag to compare images"
						aria-valuemin="0"
						aria-valuemax="100"
						aria-valuenow="50"
						aria-orientation="horizontal"
						type="button"
					>&#8203;</button>
				</div>
			</vtls-compare-slider-simple>
		</div>
	</div>
</section>

